<%= form_for :bitly, method: :post, url: ritly_path do |f|%>
NAME: <%= f.text_field :name, placeholder: "Enter Name" %> <br><br>
URL: <%= f.text_field :url, placeholder: "Website URL" %><br><br>
RNG: <%= f.text_field :random, value: @random %>
<br>
<br>
<%= f.submit class: "btn btn-primary"  %>

<% end %>